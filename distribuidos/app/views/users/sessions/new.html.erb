<div style="height:400px;width:30%;margin:0 auto;">
<h2>Iniciar Sesión</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <br/>
  <div class="field">
    <%= f.email_field :email,placeholder:"Email", autofocus: true,class:"form-control"  %>
  </div>

  <div class="field">
  <br/>
    <%= f.password_field :password, placeholder:"Contraseña",autocomplete: "off",class:"form-control" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions"> 
    <%= f.submit "Log in" %>
  </div>
<% end %>

<%= render "users/shared/links" %>
</div>